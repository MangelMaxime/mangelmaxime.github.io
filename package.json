{
    "name": "mangelmaxime.github.io",
    "scripts": {
        "clean": "shx rm -rf _site",
        "serve": "cross-env ELEVENTY_ENV=dev npx nodemon --watch .eleventy.js --watch _11ty -x 'npx eleventy --serve'",
        "prestart": "npm run clean",
        "start": "npm run serve",
        "prebuild": "npm run clean",
        "build": "cross-env ELEVENTY_ENV=dev npx eleventy",
        "build:prod": "cross-env ELEVENTY_ENV=prod npx eleventy",
        "debug": "DEBUG=* npx eleventy"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/MangelMaxime/mangelmaxime.github.io.git"
    },
    "author": "Maxime Mangel",
    "license": "MIT",
    "devDependencies": {
        "@11ty/eleventy": "^1.0.1",
        "@11ty/eleventy-plugin-inclusive-language": "^1.0.3",
        "@fec/eleventy-plugin-remark": "^3.1.1",
        "@fullhuman/postcss-purgecss": "^4.1.3",
        "cross-env": "^7.0.3",
        "cssnano": "^5.1.12",
        "eleventy-plugin-postcss": "^1.0.4",
        "eleventy-plugin-purgecss": "^0.2.1",
        "gatsby-remark-vscode": "^3.3.1",
        "html-minifier": "^4.0.0",
        "luxon": "^3.0.1",
        "nodemon": "^2.0.19",
        "postcss-cli": "^10.0.0",
        "postcss-import": "^14.1.0",
        "postcss-load-config": "^4.0.1",
        "postcss-preset-env": "^7.7.2",
        "postcss-reporter": "^7.0.5",
        "rehype-raw": "5.1.0",
        "rehype-stringify": "8.0.0",
        "remark-rehype": "8.1.0",
        "shx": "^0.3.4",
        "stylelint": "^14.9.1",
        "stylelint-config-standard": "^26.0.0"
    },
    "dependencies": {
        "collect.js": "^4.34.3"
    }
}
